@model IEnumerable<ImageGallery.Models.Image>

@{
    ViewBag.Title = "Index";
}

<h2>My pictures</h2>

<p>
    @Html.ActionLink("Add Picture", "Create")
</p>
<table>
    <tr>
        <th>Pictures</th>
    </tr>

 @foreach(var item in Model) {
        <td>
            @Html.Raw("<img style='width:100px; height:100px;' src=\"data:image/jpeg;base64,"
+ Convert.ToBase64String(item.ImageData) + "\" />")
        </td>
    }

</table>